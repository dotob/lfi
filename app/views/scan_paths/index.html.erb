<h1>Listing scan_paths</h1>

<table>
  <tr>
    <th>Path</th>
    <th>Interval</th>
    <th>Active</th>
    <th>Last visit</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scan_paths.each do |scan_path| %>
  <tr>
    <td><%= scan_path.path %></td>
    <td><%= scan_path.interval %></td>
    <td><%= scan_path.active %></td>
    <td><%= scan_path.last_visit %></td>
    <td><%= link_to 'Show', scan_path %></td>
    <td><%= link_to 'Edit', edit_scan_path_path(scan_path) %></td>
    <td><%= link_to 'Destroy', scan_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Scan path', new_scan_path_path %>
